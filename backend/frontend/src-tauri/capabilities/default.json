{
  "identifier": "default",
  "description": "Default permissions for the application",
  "platforms": ["linux", "macOS", "windows"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:webview:default",
    "core:event:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:8000/*"
        },
        {
          "url": "http://127.0.0.1:8000/*"
        }
      ]
    }
  ]
}
